CROSS = ${TARGET_CROSS}

ARCH = ${TARGET_OS}
CPU =
TARGET_VOS = posix
TARGET_OS = LINUX

TCPREFIX = ${TARGET_CROSS}
CC = ${TARGET_CC}
LD = ${TARGET_LD}
TCPOSTFIX =
DOXYPATH =

CFLAGS += -Wall -DPOSIX -D_GNU_SOURCE -DHAS_UUID -Wno-unknown-pragmas ${TARGET_CFLAGS}
ifeq (${BR2_ENDIAN},"LITTLE")
  CFLAGS += -DL_ENDIAN
else
  CFLAGS += -DB_ENDIAN
endif
LDFLAGS += -lrt -luuid ${TARGET_LDFLAGS}
